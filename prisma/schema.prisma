generator client {
  provider      = "prisma-client-js"
  output        = "../prisma/generated"
}

datasource db {
  provider  = "postgresql"
  url  	    = env("DATABASE_URL")
  directUrl = env("DIRECT_URL")
}

model User {
  id       String    @id @default(uuid()) 
  email    String    @unique
  password String
  photourl String?
  createdAt DateTime @default(now()) @map("created_at")
  name     String?
}
