generator client {
  provider      = "prisma-client-js"
  binaryTargets = ["native", "debian-openssl-1.1.x"]
}


datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id       String    @id
  email    String    @unique
  password String
  photourl String?
  createdAt DateTime @default(now()) @map("created_at")

}

