generator client {
  provider = "prisma-client-js"
    output   = "./generated/client" // Especifique o diretório de saída do Prisma Client

}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Feedback {
  id        Int        @id @default(autoincrement())
  opinion   String
  createdAt DateTime   @default(now())
}