generator client {
  provider = "prisma-client-js"
  output   = "./generated/client"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id       String    @id
  email    String    @unique
  password String
  photourl String?
  createdAt DateTime @default(now()) @map("created_at")

}

