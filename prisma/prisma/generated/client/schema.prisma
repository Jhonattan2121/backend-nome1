generator client {
  provider      = "prisma-client-js"
  output        = "./prisma/generated/client"  
    binaryTargets = ["native", "debian-openssl-1.1.x"] 
}


// Configurações do banco de dados
datasource db {
  provider = "sqlite"
  url      = "file:./dev.db" // Substitua pelo caminho do seu arquivo SQLite
}



model User {
  id       String    @id @default(uuid()) // Gera automaticamente um UUID
  email    String    @unique
  password String
  photourl String?
  createdAt DateTime @default(now()) @map("created_at")
  name     String?
}

